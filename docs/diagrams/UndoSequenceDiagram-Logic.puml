@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":RestaurantDirectoryParser" as RestaurantDirectoryParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> RestaurantDirectoryParser : parseCommand(undo)
activate RestaurantDirectoryParser

create UndoCommand
RestaurantDirectoryParser -> UndoCommand
activate UndoCommand

UndoCommand --> RestaurantDirectoryParser
deactivate UndoCommand

RestaurantDirectoryParser --> LogicManager : u
deactivate RestaurantDirectoryParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoRestaurantDirectory()
activate Model

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
